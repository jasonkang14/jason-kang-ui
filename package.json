{
  "author": "Jason Kang",
  "description": "Design System with Storybook",
  "name": "jason-kang-ui",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "files": [
    "index.css",
    "src/components/",
    "src/styles/",
    "public/icons/"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "build:css": "tailwindcss build src/index.css -o index.css",
    "release": "npm run build:css && npm publish --access public"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "src/components/index.tsx",
  "keywords": [
    "react",
    "storybook",
    "typescript",
    "vite",
    "tailwindcss",
    "design-system"
  ],
  "dependencies": {
    "@fontsource/noto-sans-kr": "^5.0.17",
    "@tanstack/react-query": "^5.17.8",
    "axios": "^1.6.5",
    "chromatic": "^10.2.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "sort-by": "^1.2.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8.0.4",
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/addon-links": "^8.0.4",
    "@storybook/addon-onboarding": "^8.0.4",
    "@storybook/blocks": "^8.0.4",
    "@storybook/react": "^8.0.4",
    "@storybook/react-vite": "^8.0.4",
    "@storybook/test": "^8.0.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.6.15",
    "postcss": "^8.4.33",
    "storybook": "^8.0.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
